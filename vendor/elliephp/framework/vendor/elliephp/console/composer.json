{
    "name": "elliephp/console",
    "description": "Symfony Console-based CLI component for ElliePHP microframework",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "ElliePHP",
            "email": "dev@elliephp.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "symfony/console": "^7.3",
        "psr/container": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "ElliePHP\\Console\\": "src/",
            "Examples\\": "examples/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ElliePHP\\Console\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
